5. Assignment Operators and Expressions
#include <stdio.h>

int main() {
    int a = 10;
    
    a += 5;  // Equivalent to a = a + 5
    printf("After += : %d\n", a);

    a -= 3;  // Equivalent to a = a - 3
    printf("After -= : %d\n", a);

    a *= 2;  // Equivalent to a = a * 2
    printf("After *= : %d\n", a);

    a /= 2;  // Equivalent to a = a / 2
    printf("After /= : %d\n", a);

    return 0;
